= form_for @file_mission do |f|
  - if @file_mission.errors.any?
    #error_explanation
      %h2= "#{pluralize(@file_mission.errors.count, "error")} prohibited this file_mission from being saved:"
      %ul
        - @file_mission.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :mission
    = f.text_field :mission
  .field
    = f.label :file
    = f.text_field :file
  .actions
    = f.submit 'Save'
